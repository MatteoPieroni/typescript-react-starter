{
  "name": "ts-react-starter",
  "version": "0.1.0",
  "description": "Typescript react starter for personal projects",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatteoPieroni/typescript-react-starter"
  },
  "keywords": [
    "eslint",
    "prettier",
    "config"
  ],
  "author": "Matteo Pieroni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MatteoPieroni/typescript-react-starter/issues"
  },
  "homepage": "https://github.com/MatteoPieroni/typescript-react-starter#readme",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --watch"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/chrome": "^0.0.101",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.0.0",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  }
}
